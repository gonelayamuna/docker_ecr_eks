pipeline {
    agent any

    stages {
        stage('Build Docker')   {
            steps {
                script {
                    def imageName = "${JOB_NAME}-${BUILD_ID}"
                    env.imageName = imageName
                    sh """
                    docker build -t ${imageName} .
                    """
                    //sh 'docker tag $JOB_NAME:v1.$BUILD_ID ${registry}:v1.$BUILD_ID'
                }
            }
        }
    }
}
